  "type": "library",
  "config": {
    "platform": {
      "php": "7.4"
    }
  },
  "keywords": [
    "deskpro",
    "api"
  ],
  "homepage": "https://github.com/deskpro/deskpro-api-client-php",
  "license": "BSD-3-Clause",
  "scripts": {
    "test": "phpunit"
  },
  "require": {
